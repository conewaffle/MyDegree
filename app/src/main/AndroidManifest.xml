<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mydegree">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".Major"
            android:parentActivityName=".Search.Search"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Plan.AddPlan"
            android:parentActivityName=".Plan.Plan"
            android:theme="@style/AppTheme" />
        <activity android:name=".Login.SignUp"/>
        <activity android:name=".Login.ForgotPassword"/>
        <activity android:name=".Login.Login">
                <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name=".CourseOverview.CourseOverview"
            android:parentActivityName=".Search.Search"
            android:theme="@style/AppTheme" />
        <activity android:name=".Profile" />
        <activity android:name=".Program">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Plan.Plan" />
        <activity android:name=".Saved.SavedItems" />
        <activity android:name=".Settings" />
        <activity android:name=".Search.Search" />
        <activity
            android:name=".ProgramDetails.ProgramDetail"
            android:parentActivityName=".Search.Search" />
        <activity android:name=".BaseActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>